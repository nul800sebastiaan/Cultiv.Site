{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "4b9560aa-7f04-4f22-bbf1-bee42fb01962",
        "SanitizeTinyMce": true
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      },
      "Hosting": {
        "Debug": false
      },	  
      "Unattended": {
        "UpgradeUnattended": true,
        "PackageMigrationsUnattended": true,
        "InstallUnattended": true,
        "UnattendedUserName": "Not Real",
        "UnattendedUserEmail": "notreal@example.com",
        "UnattendedUserPassword": "notreal123456"		
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "DeliveryApi": {
        "Enabled": true,
        "PublicAccess": true,
        "ApiKey": "ce3f50a8-b9b0-4187-9da6-58a8c5c965b4",
        "RichTextOutputAsJson": false
      }      
    }
  },
  "uSync": {
    "Settings": {
      "ImportOnFirstBoot": false
    } 
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  },
  "Kestrel": {
    "Endpoints": {
      "properties": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  }
}
